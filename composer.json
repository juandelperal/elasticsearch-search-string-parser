{
    "name": "spatie/elasticsearch-query-builder",
    "description": "Build Elasticsearch queries based of a query string",
    "keywords": [
        "spatie",
    ],
    "homepage": "https://github.com/spatie/laravel-elasticsearch-query-builder",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Vanderbist",
            "email": "alex.vanderbist@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "elasticsearch/elasticsearch": "^7.12",
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "jetbrains/phpstorm-attributes": "^1.0",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\ElasticSearchQueryBuilder\\": "src",
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\ElasticSearchQueryBuilder\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
