{
    "name": "spatie/laravel-elasticsearch-query-builder",
    "description": "Build eElasticsearch queries based of a query string",
    "keywords": [
        "spatie",
        "laravel",
        "laravel-elasticsearch-query-builder"
    ],
    "homepage": "https://github.com/spatie/laravel-elasticsearch-query-builder",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Vanderbist",
            "email": "alex.vanderbist@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "elasticsearch/elasticsearch": "^7.12",
        "illuminate/contracts": "^8.37",
        "spatie/laravel-package-tools": "^1.4.3"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "jetbrains/phpstorm-attributes": "^1.0",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\ElasticSearchQueryBuilder\\": "src",
            "Spatie\\ElasticSearchQueryBuilder\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\ElasticSearchQueryBuilder\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\ElasticSearchQueryBuilder\\ElasticSearchQueryBuilderServiceProvider"
            ],
            "aliases": {
                "ElasticSearchQueryBuilder": "Spatie\\ElasticSearchQueryBuilder\\ElasticSearchQueryBuilderFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
